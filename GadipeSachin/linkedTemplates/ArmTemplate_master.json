{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "GadipeSachin"
		},
		"LS_ASQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQLDB'"
		},
		"LS_blob01_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_blob01'"
		},
		"LS_datalake01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_datalake01'"
		},
		"LS_sqldatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sqldatabase'"
		},
		"LS_datalake01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sachindatalake01.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "GadipeSachin_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ASQLDB_connectionString": {
						"value": "[parameters('LS_ASQLDB_connectionString')]"
					},
					"LS_blob01_connectionString": {
						"value": "[parameters('LS_blob01_connectionString')]"
					},
					"LS_datalake01_accountKey": {
						"value": "[parameters('LS_datalake01_accountKey')]"
					},
					"LS_sqldatabase_connectionString": {
						"value": "[parameters('LS_sqldatabase_connectionString')]"
					},
					"LS_datalake01_properties_typeProperties_url": {
						"value": "[parameters('LS_datalake01_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}